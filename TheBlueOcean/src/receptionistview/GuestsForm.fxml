<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" fx:id="context" prefHeight="800.0" prefWidth="1200.0"
            xmlns="http://javafx.com/javafx/8.0.171" fx:controller="controller.GuestsFormController">
    <children>
        <ImageView fitHeight="800.0" fitWidth="1200.0" pickOnBounds="true">
            <image>
                <Image url="@../img/background.jpg"/>
            </image>
        </ImageView>
        <Text fill="#7a5324" layoutX="559.0" layoutY="57.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Guests">
            <font>
                <Font name="Book Antiqua Bold" size="26.0"/>
            </font>
        </Text>
        <ImageView blendMode="DARKEN" fitHeight="35.0" fitWidth="35.0" layoutX="14.0" layoutY="14.0"
                   onMouseClicked="#backOnAction" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../img/arrow_back_black_24dp.jpg"/>
            </image>
        </ImageView>
        <TableView fx:id="guestTMTable" fixedCellSize="0.0" layoutX="134.0" layoutY="124.0" opacity="0.79"
                   prefHeight="553.0" prefWidth="933.0"
                   style="-fx-background-color: #C9C9C9; -fx-border-color: #7A5324;">
            <columns>
                <TableColumn fx:id="colRoomNo" prefWidth="78.0"
                             style="-fx-background-color: #D3C1AD; -fx-border-color: #7A5324;" text="Room No"/>
                <TableColumn fx:id="colName" prefWidth="164.0"
                             style="-fx-background-color: #D3C1AD; -fx-border-color: #7A5324;" text="Name"/>
                <TableColumn fx:id="colNICNo" prefWidth="132.0"
                             style="-fx-background-color: #D3C1AD; -fx-border-color: #7A5324;" text="NIC No"/>
                <TableColumn fx:id="colPhoneNo" minWidth="0.0" prefWidth="135.0"
                             style="-fx-background-color: #D3C1AD; -fx-border-color: #7A5324;" text="Phone No"/>
                <TableColumn fx:id="colEmail" minWidth="0.0" prefWidth="152.0"
                             style="-fx-background-color: #D3C1AD; -fx-border-color: #7A5324;" text="Email"/>
                <TableColumn fx:id="colAddress" minWidth="0.0" prefWidth="149.0"
                             style="-fx-background-color: #D3C1AD; -fx-border-color: #7A5324;" text="Address"/>
                <TableColumn fx:id="colFees" minWidth="0.0" prefWidth="121.0"
                             style="-fx-background-color: #D3C1AD; -fx-border-color: #7A5324;" text="Fees"/>
            </columns>
        </TableView>
    </children>
</AnchorPane>
